{
        "_version": "1.0.0",
        "sap.app": {
                "id": "com.perezjquim.energysim.client",
                "type": "application",
                "i18n": "i18n/i18n.properties",
                "title": "{{appTitle}}",
                "description": "{{appDescription}}",
                "applicationVersion": {
                        "version": "1.0.0"
                }
        },
        "sap.ui": {
                "technology": "UI5",
                "icons": {
                        "icon": "",
                        "favIcon": "",
                        "phone": "",
                        "phone@2": "",
                        "tablet": "",
                        "tablet@2": ""
                },
                "deviceTypes": {
                        "desktop": true,
                        "tablet": true,
                        "phone": true
                }
        },
        "sap.ui5": {
                "rootView": {
                        "viewName": "com.perezjquim.energysim.client.view.App",
                        "type": "XML",
                        "async": true,
                        "id": "app"
                },
                "dependencies": {
                        "minUI5Version": "1.36.0",
                        "libs": {
                                "sap.ui.core": {},
                                "sap.m": {},
                                "sap.ui.layout": {}}},
                "contentDensities": {
                        "compact": true,
                        "cozy": true
                },
                "models": {
                        "i18n": {
                                "type": "sap.ui.model.resource.ResourceModel",
                                "settings": {
                                        "bundleName": "com.perezjquim.energysim.client.i18n.i18n"
                                }
                        },
                        "config": {
                                "type": "sap.ui.model.json.JSONModel",
                                "uri": "model/config.json"
                        },
                        "navigation": {
                                "type": "sap.ui.model.json.JSONModel",
                                "uri": "model/navigation.json"
                        },
                        "ws_logs": {
                                "type": "sap.ui.model.json.JSONModel"
                        },
                        "ws_data": {
                                "type": "sap.ui.model.json.JSONModel"
                        },
                        "ws_state": {
                                "type": "sap.ui.model.json.JSONModel",
                                "uri": "model/ws_state.json"
                        }
                },
                "resources": {
                        "js": [{
                                "uri": "libs/google_charts/loader.js"
                        }],
                        "css": [{
                                "uri": "css/style.css"
                        }]
                },
                "routing": {
                        "config": {
                                "routerClass": "sap.m.routing.Router",
                                "viewType": "XML",
                                "viewPath": "com.perezjquim.energysim.client.view",
                                "controlId": "app",
                                "controlAggregation": "pages",
                                "transition": "slide",
                                "async": true
                        },
                        "routes": [{
                                "pattern": "",
                                "name": "Home",
                                "target": ["Home"]
                        }, {
                                "pattern": "Cars",
                                "name": "Cars",
                                "target": ["Cars"]
                        }, {
                                "pattern": "Plugs",
                                "name": "Plugs",
                                "target": ["Plugs"]
                        }, {
                                "pattern": "Travels",
                                "name": "Travels",
                                "target": ["Travels"]
                        }, {
                                "pattern": "ChargingPeriods",
                                "name": "ChargingPeriods",
                                "target": ["ChargingPeriods"]
                        }, {
                                "pattern": "Logs",
                                "name": "Logs",
                                "target": ["Logs"]
                        }],
                        "targets": {
                                "Home": {
                                        "viewName": "Home"
                                },
                                "Cars": {
                                        "viewName": "Cars"
                                },
                                "Plugs": {
                                        "viewName": "Plugs"
                                },
                                "Travels": {
                                        "viewName": "Travels"
                                },
                                "ChargingPeriods": {
                                        "viewName": "ChargingPeriods"
                                },
                                "Logs": {
                                        "viewName": "Logs"
                                }
                        }
                }
        }
}